{
  "manifest_version": 2,
  "name": "ModDash",
  "description": "Quickly switch to other open tabs with just your keyboard",
  "version": "1.0.0",
  "background": {
    "persistent": true,
    "page": "./src/chrome/background.html"
  },
  "icons": {
  "128": "./src/chrome/firebase.png"
  },
  "browser_action": {
    "default_icon": "./src/chrome/firebase.png",
    "default_popup": "./src/chrome/credentials.html"
  },
  "chrome_url_overrides": {
    "newtab": "./public/dist/index.html"
  },
  "permissions": [
    "notifications",
    "background",
    "identity"
  ],
  "oauth2": {
    "client_id": "620726441303-0pq214v1roabf307l9pgunb1r218qr52.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email", "https://www.googleapis.com/auth/userinfo.profile", "https://www.googleapis.com/auth/calendar"],
    "access_type": "offline"
  },
  "key": "",
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.14.1/ https://cdn.pubnub.com/; object-src 'self' "
}
